{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "outDir": "./dist/classes",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true
    },
    "include": [
        "core/classes/*.ts"
    ]
}

